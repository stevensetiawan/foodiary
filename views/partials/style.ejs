@font-face {
    font-family: "Gotham";
    src: url(/font/GothamNarrow-Book.otf);
    font-style: normal;
    font-weight: normal;
}
@font-face {
    font-family: "Gotham";
    src: url(/font/GothamNarrow-Black.otf);
    font-style: bold;
    font-weight: bold;
}
body{
    font-family: Gotham;
    text-align: center;
    margin: 0;
}
.header{
    width: 100%; height: 30px; box-shadow: black; padding: 30px 0; border-bottom: 2px solid rgb(179, 175, 175);
}
.navbar{
    margin-left: 20px;
}
.navbar .active{
    margin: 5px 10px; text-decoration: none; text-transform: uppercase; color: rgb(241, 173, 46); font-size: 17px;
}
.navbar a{
    margin: 5px 10px; text-decoration: none; text-transform: uppercase; color: grey; font-size: 17px;
}
h1{
    font-weight: normal; font-size: 35px; margin-top: 30px;
}